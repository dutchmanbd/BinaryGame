<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_high_score"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="#008080"
    tools:context="binarygame.zxdmjr.com.binarygame.HighScore">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

    <binarygame.zxdmjr.com.binarygame.CustomTextView
        android:text="Decimal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tvDCM"
        android:gravity="center"
        android:textSize="35sp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="5dp"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:textColor="#000000" />

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:id="@+id/linearLayoutDV"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvDCM">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <binarygame.zxdmjr.com.binarygame.CustomTextView
                android:text="Easy"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tvDEasyHeader"
                android:gravity="center"
                android:layout_weight="1"
                android:textSize="25sp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="14dp"
                android:textColor="#000000" />

            <binarygame.zxdmjr.com.binarygame.CustomTextView
                android:text="Medium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tvDMediumHeader"
                android:gravity="center"
                android:layout_weight="1"
                android:textSize="25sp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="14dp"
                android:textColor="#000000" />

            <binarygame.zxdmjr.com.binarygame.CustomTextView
                android:text="Hard"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tvDHardHeader"
                android:gravity="center"
                android:layout_weight="1"
                android:textSize="25sp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="14dp"
                android:textColor="#000000" />

        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <binarygame.zxdmjr.com.binarygame.CustomTextView
                android:text="00"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tvDEasy"
                android:gravity="center"
                android:layout_weight="1"
                android:textSize="25sp"
                android:textColor="#000000"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="14dp" />

            <binarygame.zxdmjr.com.binarygame.CustomTextView
                android:text="00"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tvDMedium"
                android:gravity="center"
                android:layout_weight="1"
                android:textSize="25sp"
                android:textColor="#000000"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="14dp" />

            <binarygame.zxdmjr.com.binarygame.CustomTextView
                android:text="00"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tvDHard"
                android:gravity="center"
                android:layout_weight="1"
                android:textSize="25sp"
                android:textColor="#000000"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="14dp" />

        </LinearLayout>

    </LinearLayout>

    <binarygame.zxdmjr.com.binarygame.CustomTextView
        android:text="HexaDecimal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayoutDV"
        android:id="@+id/tvHexDCM"
        android:gravity="center"
        android:textSize="35sp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="5dp"
        android:textColor="#000000" />

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:id="@+id/linearLayoutHV"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvHexDCM">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <binarygame.zxdmjr.com.binarygame.CustomTextView
                android:text="Easy"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tvHEasyHeader"
                android:gravity="center"
                android:layout_weight="1"
                android:textSize="25sp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="14dp"
                android:textColor="#000000" />

            <binarygame.zxdmjr.com.binarygame.CustomTextView
                android:text="Hard"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tvHHardHeader"
                android:gravity="center"
                android:layout_weight="1"
                android:textSize="25sp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="14dp"
                android:textColor="#000000"/>

        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <binarygame.zxdmjr.com.binarygame.CustomTextView
                android:text="00"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tvHEasy"
                android:gravity="center"
                android:layout_weight="1"
                android:textSize="25sp"
                android:textColor="#000000"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="14dp" />
            <binarygame.zxdmjr.com.binarygame.CustomTextView
                android:text="00"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tvHHard"
                android:gravity="center"
                android:layout_weight="1"
                android:textSize="25sp"
                android:textColor="#000000"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="14dp" />

        </LinearLayout>

    </LinearLayout>

    <binarygame.zxdmjr.com.binarygame.CustomTextView
        android:text="Octal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayoutHV"
        android:id="@+id/tvOCT"
        android:gravity="center"
        android:textSize="35sp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="5dp"
        android:textColor="#000000" />

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:id="@+id/linearLayoutOV"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvOCT">

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <binarygame.zxdmjr.com.binarygame.CustomTextView
                android:text="Easy"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tvOEasyHeader"
                android:gravity="center"
                android:layout_weight="1"
                android:textSize="25sp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="14dp"
                android:textColor="#000000" />

            <binarygame.zxdmjr.com.binarygame.CustomTextView
                android:text="Medium"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tvOMediumHeader"
                android:gravity="center"
                android:layout_weight="1"
                android:textSize="25sp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="14dp"
                android:textColor="#000000" />

            <binarygame.zxdmjr.com.binarygame.CustomTextView
                android:text="Hard"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tvOHardHeader"
                android:gravity="center"
                android:layout_weight="1"
                android:textSize="25sp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="14dp"
                android:textColor="#000000" />

        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <binarygame.zxdmjr.com.binarygame.CustomTextView
                android:text="00"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tvOEasy"
                android:gravity="center"
                android:layout_weight="1"
                android:textSize="25sp"
                android:textColor="#000000"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="14dp" />
            <binarygame.zxdmjr.com.binarygame.CustomTextView
                android:text="00"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tvOMedium"
                android:gravity="center"
                android:layout_weight="1"
                android:textSize="25sp"
                android:textColor="#000000"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="14dp" />
            <binarygame.zxdmjr.com.binarygame.CustomTextView
                android:text="00"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tvOHard"
                android:gravity="center"
                android:layout_weight="1"
                android:textSize="25sp"
                android:textColor="#000000"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_marginTop="14dp" />

        </LinearLayout>

    </LinearLayout>

        </LinearLayout>

    </ScrollView>


</RelativeLayout>
